{
  "name": "eslint-config-kotori",
  "version": "0.6.0",
  "description": "ESLint shareable config for me.",
  "main": "index.js",
  "scripts": {
    "pretest": "node test/install.js",
    "test": "npm run test:js && npm run test:react && npm run test:ts && npm run test:vue",
    "test:js": "cd test/js && eslint --ext .js ./",
    "test:react": "cd test/react && eslint --ext .js,.jsx ./",
    "test:ts": "cd test/ts && eslint --ext .ts,.tsx,.js ./",
    "test:vue": "cd test/vue && eslint --ext .js,.vue ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kokororin/eslint-config-kotori.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config"
  ],
  "author": "kokororin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kokororin/eslint-config-kotori/issues"
  },
  "homepage": "https://github.com/kokororin/eslint-config-kotori#readme",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-vue": "^8.0.3",
    "ptils": "^0.3.0",
    "vue-eslint-parser": "^8.0.1"
  },
  "peerDependencies": {
    "eslint": ">=6.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "typescript": "^4.0.0"
  },
  "files": [
    "auto.js",
    "index.js",
    "react.js",
    "typescript.js",
    "vue.js"
  ]
}
